    <owl:NamedIndividual rdf:about="http://www.zhewang.de/footballnet#Tottenham_Hotspur_Football_Club">
        <rdf:type rdf:resource="http://www.zhewang.de/footballnet#Football_Club"/>
        <belongs_to rdf:resource="http://www.zhewang.de/footballnet#English_Premier_League"/>
