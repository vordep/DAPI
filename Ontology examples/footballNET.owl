    <!-- http://www.zhewang.de/footballnet#Everton_Football_Club -->

    <owl:NamedIndividual rdf:about="http://www.zhewang.de/footballnet#Everton_Football_Club">
        <rdf:type rdf:resource="http://www.zhewang.de/footballnet#Football_Club"/>
